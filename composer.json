{
    "name": "alex-kalanis/kw_mime",
    "description": "Determine file mime type in KWCMS",
    "keywords": [
        "mime",
        "mimetype",
        "files",
        "file"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_paths": ">=4.0 <5",
        "php": ">=7.4.0"
    },
    "require-dev": {
        "alex-kalanis/kw_files": ">=4.0 <5",
        "alex-kalanis/kw_storage": ">=5.0 <6",
        "shipmonk/composer-dependency-analyser": "^1.4",
        "ext-fileinfo": "*",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": ">=7.2 <=9"
    },
    "suggest": {
        "alex-kalanis/kw_files": "Accessing stored entries as files on some volume",
        "alex-kalanis/kw_storage": "Universal storage library for store data as key-value pairs",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_mime\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
